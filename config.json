{
  "theme": {
    "primary_color": "#FFD700",
    "background_color": "#1A1A1A",
    "font_family": "'Cutive Mono', monospace"
  },
  "camera_ui": {
    "show_clapperboard_icon": true,
    "enable_zoom_on_activate": true
  },
  "effects": {
    "golden_earthy": {
      "type": "COLOR_GRADING",
      "name": "Golden Earthy",
      "css_filter": "sepia(0.3) saturate(1.2) contrast(0.9) brightness(0.9)"
    },
    "slow_zoom": {
      "type": "ZOOM",
      "from": 1,
      "to": 1.05,
      "duration_ms": 4000,
      "timing_function": "cubic-bezier(0.4, 0, 0.2, 1)"
    }
  },
  "post_processing": {
    "stabilization": {
      "enabled": false,
      "strength": "medium"
    }
  },
  "solvers": {
    "FACE_DETECTION_CLOSE_UP": {
      "model": "blazeface",
      "target_box_scale": 0.8,
      "feedback_prompts": {
        "too_far": "Move closer to the subject",
        "too_close": "Move back a little",
        "off_center": "Center the subject's face",
        "good_job": "Perfect framing!"
      }
    },
    "POSE_DETECTION_MEDIUM": {
      "model": "movenet",
      "target_box_scale": 0.9,
      "feedback_prompts": {
        "too_far": "Move closer",
        "too_close": "Move back",
        "good_job": "Great! Hold it there."
      }
    }
  }
}
